{
    "manifest_version":2,
    "name":"WebCustomiser",
    "version":"0.1",
    "description":"Web customiser",
    "content_scripts":[
        {
            "matches":["*://vk.com/*"],
            "css":["vk/style.css", "vk/my.css"]
        },
        {
            "matches":["*://www.google.ru/*"],
            "css":["google/style.css", "google/my.css"]
        },
        {
            "matches":["*://tongji.baidu.com/**"],
            "css":["tongji_baidu/my.css"]
        },
	{
	    "matches":["*://mp.weixin.qq.com/**"],
	    "css":["wechat/my.css"]
	},
	{
            "matches":["*://open.weixin.qq.com/**"],
            "css":["wechat2/my.css"]
        }
    ]
}
